<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    android:orientation="vertical"
    android:background="@color/smsp_white_color"
    android:layout_margin="5dp"
    >
    <LinearLayout
        android:id="@+id/linearLayout_root_category"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/styele_imageView3"
                android:layout_width="190dp"
                android:layout_height="170dp"
                android:src="@drawable/image_shop_13"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/category_image"
                android:layout_width="190dp"
                android:layout_height="170dp"
                android:src="@drawable/image_shop_13"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/category_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="#090808"
                android:textColor="#ffffff"
                android:textSize="15sp"
                app:layout_constraintStart_toStartOf="@+id/category_image"
                app:layout_constraintTop_toTopOf="@+id/category_image"
                tools:text="SEATS 5" />

            <TextView
                android:id="@+id/category_name"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="Style and Comfort"
                android:padding="5dp"
                android:textColor="#0C0C0C"
                android:textSize="15sp"
                android:background="@drawable/rectangle"
                app:layout_constraintEnd_toEndOf="@+id/category_image"
                app:layout_constraintStart_toStartOf="@+id/category_image"
                app:layout_constraintTop_toBottomOf="@+id/category_image" />

            <ImageView
                android:id="@+id/style_imageView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:layout_constraintBottom_toBottomOf="@+id/category_name"
                app:layout_constraintEnd_toEndOf="@+id/category_name"
                app:layout_constraintTop_toTopOf="@+id/category_name"
                app:srcCompat="@drawable/ic_keyboard_arrow_right_black_24dp"
                tools:ignore="VectorDrawableCompat" />
        </androidx.constraintlayout.widget.ConstraintLayout>
